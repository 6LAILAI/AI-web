{
  "version": 2,
  "builds": [
    {
      "src": "src",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/src/*",
      "destination": "/:splat",
      "statusCode": 200
    },
    {
      "source": "/*",
      "destination": "/",
      "statusCode": 200
    }
  ]
}