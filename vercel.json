{
  "version": 2,
  "builds": [
    {
      "src": "src",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "redirects": [
    {
      "from": "/src/*",
      "to": "/:splat",
      "status": 200
    },
    {
      "from": "/*",
      "to": "/",
      "status": 200
    }
  ]
}